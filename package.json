{
  "name": "vite-svelte-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:remote": "vite --host",
    "build": "pwa-assets-generator && vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "lint": "eslint src",
    "lint:styles": "stylelint --config .stylelintrc \"src/**/*.{css,scss}\"",
    "format": "eslint --fix src",
    "format:prettier": "prettier --write src/**/{*.svelte,*.ts,*.scss}",
    "format:stylelint": "stylelint --config .stylelintrc \"src/**/*.{css,scss}\" --fix",
    "prepare": "husky",
    "generate-pwa-assets": "pwa-assets-generator"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@picocss/pico": "^2.0.6",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.6",
    "@tsconfig/svelte": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitest/coverage-v8": "^3.0.2",
    "@vitest/ui": "^3.0.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-css-order": "^2.1.0",
    "prettier-plugin-svelte": "^3.3.3",
    "sass": "^1.83.4",
    "stylelint": "^16.13.2",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-prettier": "^5.0.2",
    "svelte": "^5.19.0",
    "svelte-check": "^4.1.4",
    "svelte-i18n": "^4.0.1",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vite": "^6.0.10",
    "vite-plugin-mkcert": "^1.17.6",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^3.0.2"
  }
}